@page
@model Homework4Umbraco.Pages.SerialNumbers.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
           
            <th>
                @Html.DisplayNameFor(model => model.SerialNumber[0].SerialNumberID)
            </th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.SerialNumber) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SerialNumberID)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.SerialNumberID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.SerialNumberID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.SerialNumberID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
